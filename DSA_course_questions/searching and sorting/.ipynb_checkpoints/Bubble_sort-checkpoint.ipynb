   "source": [
    "def Bubblesort(li):\n",
    "    length = len(li)\n",
    "    for i in range(0,length):\n",
    "        for j in range(0,length-i-1):\n",
    "            if li[j] > li[j+1]:\n",
    "                li[j],li[j+1] = li[j+1],li[j]\n",
    "    return li\n"
   ]
  },
